<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Title for the application. [CHAR LIMIT=15] -->
    <string name="app_name">Flowery Quiz</string>

    <!-- Hint text display in the empty field for the user's name [CHAR LIMIT=20] -->
    <string name="name">Your name</string>

    <!-- Text below the title to greet a user and invite him to do the quiz. [CHAR LIMIT=NONE] -->
    <string name="app_greeting">How much do you know about flowers and plants? Let\'s find out!
        \n\nFor starters please tell us your name:</string>
    <!-- Text below the title to greet a user and invite him to do the quiz. [CHAR LIMIT=NONE] -->
    <string name="good_luck">Good Luck!</string>

    <!-- First quiz question. [CHAR LIMIT=NONE] -->
    <string name="question_1">1. What flowers blooming in spring are significant part of Japanese culture?</string>
    <!-- First quiz question - answer a. [CHAR LIMIT=NONE] -->
    <string name="question_1_a">a. Cherry blossoms</string>
    <!-- First quiz question - answer b. [CHAR LIMIT=NONE] -->
    <string name="question_1_b">b. Orchids</string>
    <!-- First quiz question - answer c. [CHAR LIMIT=NONE] -->
    <string name="question_1_c">c. Azaleas (Rhododendrons)</string>

    <!-- Second quiz question. [CHAR LIMIT=NONE] -->
    <string name="question_2">2. By which historic country tulips were introduced to Europe?</string>
    <!-- Second quiz question - answer a. [CHAR LIMIT=NONE] -->
    <string name="question_2_a">a. The Qing Empire (Qing Dynasty)</string>
    <!-- Second quiz question - answer b. [CHAR LIMIT=NONE] -->
    <string name="question_2_b">b. The Low Countries (old Netherlands region)</string>
    <!-- Second quiz question - answer c. [CHAR LIMIT=NONE] -->
    <string name="question_2_c">c. The Ottoman Empire (Turkish Empire)</string>

    <!-- Third quiz question. [CHAR LIMIT=NONE] -->
    <string name="question_3">3. Which flowers are edible?</string>
    <!-- Third quiz question - answer a. [CHAR LIMIT=NONE] -->
    <string name="question_3_a">a. Tulips</string>
    <!-- Third quiz question - answer b. [CHAR LIMIT=NONE] -->
    <string name="question_3_b">b. Marigolds</string>
    <!-- Third quiz question - answer c. [CHAR LIMIT=NONE] -->
    <string name="question_3_c">c. Convallarias (&quot;Lilies of the valley&quot;)</string>
    <!-- Third quiz question - answer d. [CHAR LIMIT=NONE] -->
    <string name="question_3_d">d. Violets</string>

    <!-- Fourth quiz question. [CHAR LIMIT=NONE] -->
    <string name="question_4">4. What flower genus is shown on picture?</string>
    <!-- Fourth quiz question - answer a. [CHAR LIMIT=NONE] -->
    <string name="question_4_a">a. Peony</string>
    <!-- Fourth quiz question - answer b. [CHAR LIMIT=NONE] -->
    <string name="question_4_b">b. Trollius</string>
    <!-- Fourth quiz question - answer c. [CHAR LIMIT=NONE] -->
    <string name="question_4_c">c. Rose</string>

    <!-- Fifth quiz question. [CHAR LIMIT=NONE] -->
    <string name="question_5">5. Which of the following flowers are fragrant?</string>
    <!-- Fifth quiz question - answer a. [CHAR LIMIT=NONE] -->
    <string name="question_5_a">a. Lilies</string>
    <!-- Fifth quiz question - answer b. [CHAR LIMIT=NONE] -->
    <string name="question_5_b">b. Hyacinths</string>
    <!-- Fifth quiz question - answer c. [CHAR LIMIT=NONE] -->
    <string name="question_5_c">c. Hydrangeas</string>
    <!-- Fifth quiz question - answer d. [CHAR LIMIT=NONE] -->
    <string name="question_5_d">d. Daisies</string>
    <!-- Fifth quiz question - answer e. [CHAR LIMIT=NONE] -->
    <string name="question_5_e">e. Freesias</string>

    <!-- Sixth quiz question. [CHAR LIMIT=NONE] -->
    <string name="question_6">6. Leaf of which tree is depicted on Canadian flag?</string>
    <!-- Sixth quiz question - answer a. [CHAR LIMIT=NONE] -->
    <string name="question_6_a">Write your answer here</string>

    <!-- Seventh quiz question. [CHAR LIMIT=NONE] -->
    <string name="question_7">7. Which of the following plants genera are succulents?</string>
    <!-- Seventh quiz question - answer a. [CHAR LIMIT=NONE] -->
    <string name="question_7_a">a. Fern</string>
    <!-- Seventh quiz question - answer b. [CHAR LIMIT=NONE] -->
    <string name="question_7_b">b. Cactuses</string>
    <!-- Seventh quiz question - answer c. [CHAR LIMIT=NONE] -->
    <string name="question_7_c">c. Orchid</string>
    <!-- Seventh quiz question - answer d. [CHAR LIMIT=NONE] -->
    <string name="question_7_d">d. Agave</string>

    <!-- Text displayed in the Button for submitting answers [CHAR LIMIT=12] -->
    <string name="submit_button">Submit Answers</string>

    <!-- Initial text displayed below Submit button [CHAR LIMIT=NONE] -->
    <string name="initial_result_text" translatable="false"> </string>

    <!-- Hint for question 3 [CHAR LIMIT=NONE] -->
    <string name="hint_q3">Make sure you answered correctly ;)</string>

    <!-- Hint for question 5 [CHAR LIMIT=NONE] -->
    <string name="hint_q5">Are they..?</string>

    <!--
      Message displayed when name field is not filled [CHAR LIMIT=NONE]
        -->
    <string name="name_not_given_message">You haven\'t introduced. Please insert your name first.</string>

